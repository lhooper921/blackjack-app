   
    {{> header}}
   
    <!-- Cards Background image -->
    <div class="container-fluid">
        <div class="hero-image">
            
            <div class="container" id="leader-table">
                <div class="row">
                    <div class="col-12">
                    <table class="table table-hover">
                        <thead>
                          <tr>
                            <th scope="col">Rank</th>
                            <th scope="col">Name</th>
                            <th scope="col">Username</th>
                            <th scope="col">Score</th>
                          </tr>
                        </thead>
                        <tbody>
<!-- ==============================================================GENERATED BY HANDLEBARS================================================================================== -->
                          {{#each user}}
                          <tr>
                            <th scope="row"></th>
                            <td>{{this.dataValues.userName}}</td>
                            <td>{{this.dataValues.chips}}</td>
                            <td>{{this.dataValues.gamesPlayed}}</td>
                          </tr>
                          {{/each}}
                        </tbody>
                      </table>
                </div>
            </div>
        </div>
        </div>
    </div>

     {{> footer}}