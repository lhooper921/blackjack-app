   
    {{> header}}
   
    <!-- Cards Background image -->
  <!-- Cards Background image -->
<div class="container-fluid" id="index-page">
    <img src="../public/images/cards.jpg" class="img-fluid " id="index-background">
       <!-- Logo -->
       <div class="row">
      <div class="xs-col-2 md-col-8" id="logo">
        <img src="/public/images/blackjack_logo.png"  alt="" loading="lazy">
            </div>
            </div>
            
            <div class="container" id="leader-table">
                <div class="row">
                    <div class="col-12">
                    <table class="table table-hover">
                        <thead>
                          <tr>
                            <th scope="col">Rank</th>
                            <th scope="col">Player</th>
                            <th scope="col">Chips</th>
                            <th scope="col">Games Played</th>
                          </tr>
                        </thead>
                        <tbody>
<!-- ==============================================================GENERATED BY HANDLEBARS================================================================================== -->
                          {{#each user}}
                          <tr>
                            <td>{{@index}}</td>
                            <td>{{this.dataValues.userName}}</td>
                            <td>{{this.dataValues.chips}}</td>
                            <td>{{this.dataValues.gamesPlayed}}</td>
                          </tr>
                          {{/each}}
                        </tbody>
                      </table>
                </div>
            </div>
        </div>
        </div>
      </div>
    </div>
  </div>
</div>

{{> footer}}